<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
             http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-3.2.xsd">
       <context:annotation-config/>
 <bean id="payment" class="com.accenture.bean.Payment"/>
  <bean id="javaOrderBean" class="com.accenture.bean.Order">
  <property name="item" value="Java Book"/>
   <property name="price" value="Rs 225"/>
   </bean>
   
  <bean id="oracleOrderBean" class="com.accenture.bean.Order">
  <property name="item" value="Oracle Book"/>
  <property name="price" value="Rs 375"/>
   </bean>
   </beans>